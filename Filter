export function ProductCardList(): Override {
    return {
        searchTerm: data.searchTerm,
        onItemTap(item: any) {
            data.selectedItem = item
        },
    }
}

export function Button(): Override {
    return {
        onTap() {
            ;(data.searchTerm = ""),
                (data.searchTerm =
                    data.Gezondheid == true
                        ? data.searchTerm + "s/aPG8&  "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Health1Checked == true
                        ? data.searchTerm + "6#*wX5* "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Health2Checked == true
                        ? data.searchTerm + "'P74hT?X  "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Health3Checked == true
                        ? data.searchTerm + "8Bh;jH<A  "
                        : data.searchTerm),
                (data.searchTerm =
                    data.DevelopmentChecked == true
                        ? data.searchTerm + "pg*Rm&4d "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Development1Checked == true
                        ? data.searchTerm + "Jp'vE9YA "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Development2Checked == true
                        ? data.searchTerm + "T)9r~w$8 "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Development3Checked == true
                        ? data.searchTerm + "&nKz8X}+  "
                        : data.searchTerm),
                (data.searchTerm =
                    data.AnimalChecked == true
                        ? data.searchTerm + "4<5XzgS$ "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Animal1Checked == true
                        ? data.searchTerm + "h&V7,X:  "
                        : data.searchTerm),
                (data.searchTerm =
                    data.ClimateChecked == true
                        ? data.searchTerm + "EWj]D3c "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Climate1Checked == true
                        ? data.searchTerm + "@>ARrz49 "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Climate2Checked == true
                        ? data.searchTerm + "x;pT3~N]  "
                        : data.searchTerm),
                (data.searchTerm =
                    data.FutureChecked == true
                        ? data.searchTerm + "tNX9e?< "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Future1Checked == true
                        ? data.searchTerm + "G#uFt3c2 "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Future2Checked == true
                        ? data.searchTerm + "3S+5]acZ "
                        : data.searchTerm),
                (data.searchTerm =
                    data.Future3Checked == true
                        ? data.searchTerm + "gA)9:w% "
                        : data.searchTerm),
                (data.searchTerm =
                    data.CharityChecked == true
                        ? data.searchTerm + "7XnWU$_: "
                        : data.searchTerm),
                (data.searchTerm =
                    data.FundChecked == true
                        ? data.searchTerm + "$9mh{E%w "
                        : data.searchTerm),
                (data.searchTerm =
                    data.ANBIChecked == true
                        ? data.searchTerm + " &7#%M@g3 "
                        : data.searchTerm),
                (data.searchTerm =
                    data.InterventionChecked == true
                        ? data.searchTerm + "G6y7]~zj "
                        : data.searchTerm),
                (data.searchTerm =
                    data.PolicyChecked == true
                        ? data.searchTerm + "^7f~-$Zk "
                        : data.searchTerm),
                (data.searchTerm =
                    data.InnovationChecked == true
                        ? data.searchTerm + "pg~W6&(g "
                        : data.searchTerm)
        },
    }
}

export function CharityPage(): Override {
    return {
        concreteImpact: data.selectedItem
            ? data.selectedItem.concreteImpact
            : "",
        concreteImpactUnit: data.selectedItem
            ? data.selectedItem.concreteImpactUnit
            : "",
        relativeImpact: data.selectedItem
            ? data.selectedItem.relativeImpact
            : "",
        relativeImpactUnit: data.selectedItem
            ? data.selectedItem.relativeImpactUnit
            : "",
        anbi: data.selectedItem ? data.selectedItem.anbi : "",
        beschrijving: data.selectedItem ? data.selectedItem.Beschrijving : "",
        category: data.selectedItem ? data.selectedItem.category : "",
        evaluatingOrganisation: data.selectedItem
            ? data.selectedItem.evaluatingOrganisation
            : "",
        fundCharity: data.selectedItem ? data.selectedItem.fundCharity : "",
        methode: data.selectedItem ? data.selectedItem.methode : "",
        nameCopy: data.selectedItem ? data.selectedItem.nameCopy : "",
        subcategory: data.selectedItem ? data.selectedItem.subcategory : "",
        dataQuality: data.selectedItem ? data.selectedItem.dataQuality : "",
        colorData: data.selectedItem ? data.selectedItem.colorData : "",
        zekerheidImpact: data.selectedItem
            ? data.selectedItem.zekerheidImpact
            : "",
    }
}
