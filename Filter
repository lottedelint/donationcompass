import { Override, Data, Color } from "framer"
import * as React from "react"

const data = Data({
    selectedItem: null,
    searchTerm: "",

    Gezondheid: false,
    Health1Checked: false,
    Health2Checked: false,
    Health3Checked: false,

    DevelopmentChecked: false,
    Development1Checked: false,
    Development2Checked: false,
    Development3Checked: false,

    AnimalChecked: false,
    Animal1Checked: false,

    ClimateChecked: false,
    Climate1Checked: false,
    Climate2Checked: false,

    FutureChecked: false,
    Future1Checked: false,
    Future2Checked: false,
    Future3Checked: false,

    ANBIChecked: false,
    CharityChecked: false,
    FundChecked: false,

    PolicyChecked: false,
    InterventionChecked: false,
    InnovationChecked: false,

    Negativehealth: false,
})

export function Mainhealth(): Override {
    return {
        onToggle: () => {
            data.Gezondheid = !data.Gezondheid
            data.Negativehealth = true
        },
    }
}

export function Subhealth1(): Override {
    return {
        onToggle() {
            data.Health1Checked = !data.Health1Checked
        },
    }
}

export function Subhealth2(): Override {
    return {
        onToggle() {
            data.Health2Checked = !data.Health2Checked
        },
    }
}

export function Subhealth3(): Override {
    return {
        onToggle() {
            data.Health3Checked = !data.Health3Checked
        },
    }
}

export function MainDevelopment(): Override {
    return {
        onToggle: () => {
            data.DevelopmentChecked = !data.DevelopmentChecked
        },
    }
}

